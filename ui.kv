<MainScreen>:
    canvas:
        Color:
            rgb: list(map(lambda x: float(x)/255, [90, 12, 102]))
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        ClockLabel:
            id: clock_label
            text: self.get_time()
            font_size: 200
        Label:
            text: 'Hey'
            size_hint_x: None
            width: 300